{
    "name": "ts-node-boilerplate",
    "productName": "TS-Node Boilerplate",
    "version": "1.0.0",
    "description": "A TS-Node & EsLint boilerplate entirely configured to suit my standards.",
    "repository": "https://github.com/yoratoni/ts-node-boilerplate",
    "author": "yoratoni <yoratoni.dev@gmail.com>",
    "license": "MIT",
    "private": true,
    "scripts": {
        "v-bump": "yarn version --patch",
        "main": "ts-node src/scripts/main.ts",
        "lint": "eslint . --ext .ts,.tsx",
        "lint:fix": "eslint . --ext .ts,.tsx --fix",
        "check-types": "tsc --noEmit --skipLibCheck --incremental false"
    },
    "dependencies": {
        "date-and-time": "^3.4.0",
        "dedent-js": "^1.0.1",
        "dotenv": "^16.4.5",
        "minimist": "^1.2.8",
        "node-os-utils": "^1.3.7",
        "ts-node": "^10.9.2",
        "winston": "^3.13.0"
    },
    "devDependencies": {
        "@total-typescript/ts-reset": "^0.5.1",
        "@types/minimist": "^1.2.5",
        "@types/node": "^20.14.10",
        "@types/node-os-utils": "^1.3.4",
        "@typescript-eslint/eslint-plugin": "^7.15.0",
        "@typescript-eslint/parser": "^7.15.0",
        "eslint": "^8.57.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.5.3"
    }
}