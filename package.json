{
    "name": "ts-node-boilerplate",
    "productName": "TS-Node Boilerplate",
    "version": "1.0.0",
    "description": "A TS-Node & BiomeJS boilerplate entirely configured to suit my standards.",
    "repository": "https://github.com/yoratoni/ts-node-boilerplate",
    "author": "yoratoni <yoratoni.dev@gmail.com>",
    "license": "MIT",
    "private": true,
    "scripts": {
        "v-bump": "yarn version --patch",
        "main": "ts-node src/scripts/main.ts",
        "lint": "biome lint .",
        "lint:fix": "biome lint . --apply",
        "format": "biome format .",
        "format:fix": "biome format --write .",
        "check-types": "tsc --noEmit --skipLibCheck --incremental false"
    },
    "dependencies": {
        "date-and-time": "^3.5.0",
        "dedent-js": "^1.0.1",
        "dotenv": "^16.4.5",
        "minimist": "^1.2.8",
        "node-os-utils": "^1.3.7",
        "ts-node": "^10.9.2",
        "winston": "^3.14.2"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.9.0",
        "@types/minimist": "^1.2.5",
        "@types/node": "^22.5.4",
        "@types/node-os-utils": "^1.3.4",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.6.2"
    }
}